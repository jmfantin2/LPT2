    grammar org.example.domainmodel.Domainmodel with org.eclipse.xtext.common.Terminals
     
	generate domainmodel "http://www.xtext.org/example/mydsl/Domainmodel"

    Model:
    	tag += Tag*
    ;
     
    Doctype:
     	'!DOCTYPE html';
     	
    HtmlObject:
     	Doctype;
     	
    Tag:
     	'<' HtmlObject '>';
